{
  "name": "ynloultratech/graphql-media-service",
  "description": "Serve and manage files using a GraphQL API",
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "YNLO-Ultratech Development Team",
      "email": "developer@ynloultratech.com",
      "homepage": "https://github.com/ynloultratech"
    }
  ],
  "keywords": [
    "media-service",
    "graphql"
  ],
  "autoload": {
    "psr-4": {
      "Ynlo\\GraphQLMediaServiceBundle\\": "src/"
    }
  },
  "require": {
    "php": "^7.1.3",
    "symfony/form": "^3.4 || ^4.4",
    "symfony/framework-bundle": "^3.4 || ^4.4",
    "doctrine/annotations": "^1.5",
    "doctrine/common": "^2.8",
    "ynloultratech/graphql-bundle": "^1.4",
    "spatie/url-signer": "^1.0",
    "symfony/mime": "^3.4 || ^4.4"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "bin-dir": "bin",
    "preferred-install": "dist"
  }
}
